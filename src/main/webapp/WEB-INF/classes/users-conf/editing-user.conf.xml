<?xml version="1.0" encoding="ISO-8859-1"?>
<root>
 <preInsert excludeRel="son,lowerbrother,upperbrother" suggestedRel="none">
  <macroarea>
   <sezione name="identificazione account" opened="yes">
    <elemento input_type="text" class="Long" name="nome">/user/@name</elemento>
    <elemento input_type="text" class="Long" name="cognome">/user/@lastName</elemento>
    <elemento input_type="text" class="Long" name="account">/user/@account</elemento>
    <elemento input_type="text" class="Long" name="email">/user/@email</elemento>
    <elemento input_type="text" class="Long" name="pwd">/user/@pwd</elemento>
    <elemento input_type="text" class="Long" name="id">/user/@id</elemento>
    <elemento input_type="text" class="Long" name="ruolo">/user/@role</elemento>
   </sezione>
   <sezione name="archivi" opened="yes">
    <elemento type="multi_group" prefix="/user/archive">
     <elemento input_type="text" class="Long" name="nome">/user/archive/text()</elemento>
     <elemento input_type="text" class="Long" name="alias">/user/archive/@alias</elemento>
     <elemento input_type="text" class="Long" name="ruolo">/user/archive/@role</elemento>
    </elemento>
   </sezione>
  </macroarea>
 </preInsert>
 <fixedValues>
  <elemento value="user">thePne</elemento>
  <elemento value="inserimento">/user/processinfo/list/item/text()</elemento>
  <elemento value="@coluiCheInserisce@">/user/processinfo/list/item/persname/text()</elemento>
  <elemento value="@quandoColuiInserisce@">/user/processinfo/list/item/date/text()</elemento>
  <elemento value=".">/user/@id</elemento>
  <elemento value="IT">/user/did/unitid[@encodinganalog='ISAD 3-1-1 reference code']/@countrycode</elemento>
 </fixedValues>
 <param>
  <elemento id="campi_obbligatori">.c.did.unittitle[@encodinganalog='ISAD 3-1-2 title'].text()</elemento>
  <elemento id="codice_identificativo">/user/did/unitid[@encodinganalog='ISAD 3-1-1 reference code']/text()</elemento>
  <elemento id="riordina">
   <opzione value="/user/did/unitid">codice identificativo logico</opzione>
   <opzione value="/user/did/unittitle">denominazione</opzione>
   <opzione value="/user/did/unittitle/unitdate/@normal">data (estremo iniziale)</opzione>
   <opzione value="XML(xpart:/user/did/unittitle/unitdate/@normal)(part:9:8)">data (estremo finale)</opzione>
   <opzione value="/user/did/unittitle/num/emph:n">numero ordinamento provvisorio (scheda unità)</opzione>
   <opzione value="/user/did/unittitle/num:n">numero ordinamento definitivo (scheda unità)</opzione>
   <opzione value="/user/descgrp/odd/p">altra segnatura (o antica o precedente)</opzione>
   <opzione value="/user/descgrp/index/indexentry/name">titolario di classificazione (scheda unità)</opzione>
   <opzione value="/user/did/physloc">collocazione</opzione>
  </elemento>
  <elemento id="clona">
   <opzione action="delete" value="">/user/processinfo/list/item</opzione>
   <opzione action="append" value="elemento copiato">/user/processinfo/list/item/text()</opzione>
   <opzione action="append" value="systemdate">/user/processinfo/list/item/date/text()</opzione>
   <opzione action="append" value="username">/user/processinfo/list/item/persname/text()</opzione>
   <opzione action="append" value=" Clonato ">/user/clonato/text()</opzione>
   <opzione action="replace" value=".">/user/@id</opzione>
  </elemento>
 </param>
 <docEdit>
  <macroarea>
   <sezione name="identificazione utente" opened="yes">
    <elemento input_type="text" class="Long" name="nome">/user/@name</elemento>
    <elemento input_type="text" class="Long" name="cognome">/user/@lastName</elemento>
    <elemento input_type="text" class="Long" name="account">/user/@account</elemento>
    <elemento input_type="text" class="Long" name="email">/user/@email</elemento>
    <elemento input_type="text" class="Long" name="pwd">/user/@pwd</elemento>
    <elemento input_type="text" class="Long" name="id">/user/@id</elemento>
    <elemento input_type="text" class="Long" name="ruolo">/user/@role</elemento>
    <!-- <user language="IT" lastName="xDams - open source" account="xdams.org" email="info@xdams.org" name="Admin" pwd="0938c220390110ae9ebf07445f07ee19" id="admin" role="ROLE_ADMIN"> -->
   </sezione>
   <sezione name="archivi" opened="yes">
    <elemento type="multi_group" prefix="/user/archive">
     <elemento input_type="text" class="Long" name="nome">/user/archive/text()</elemento>
     <elemento input_type="text" class="Long" name="alias">/user/archive/@alias</elemento>
     <elemento input_type="text" class="Long" name="ruolo">/user/archive/@role</elemento>
    </elemento>
   </sezione>
  </macroarea>
  <sezione name="campiHidden">
   <elemento>/user/did/unitid[@encodinganalog='ISAD 3-1-1 reference code']/@countrycode</elemento>
   <elemento>/user/did/unitid[@encodinganalog='ISAD 3-1-1 reference code']/@repositorycode</elemento>
   <elemento>/user/did/unitid[@encodinganalog='ISAD 3-1-1 reference code']/abbr/text()</elemento>
   <elemento>/user/did/unitid[@encodinganalog='ISAD 3-1-1 reference code']/@identifier</elemento>
   <elemento>/user/@id</elemento>
  </sezione>
 </docEdit>
</root>