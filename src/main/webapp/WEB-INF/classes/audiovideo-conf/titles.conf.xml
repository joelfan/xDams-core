<?xml version="1.0" encoding="ISO-8859-1"?>
<root>
 <titleManager>
  <sezione name="title">
   <elemento mode="html" html="strong" format_after="&#160;">0-[@titProp@]</elemento>
   <elemento mode="html" html="em"> [@titProp@]-[@data@]</elemento>
   <elemento mode="html" format_before="&#160;-&#160;">[@data@]-[@aut@]</elemento>
  </sezione>
  <sezione name="hierBrowser">
   <elemento mode="html" html="strong" format_after="&#160;">0-[@titProp@]</elemento>
   <elemento mode="html" html="em"> [@titProp@]-[@data@]
      <substitutor> 
   <regex>¢</regex>
   <replacement>&#160;</replacement>
   </substitutor>
   </elemento>
   <elemento mode="html" format_before="&#160;-&#160;">[@data@]-[@aut@]</elemento>
  </sezione>
  <sezione name="hierTitle">
